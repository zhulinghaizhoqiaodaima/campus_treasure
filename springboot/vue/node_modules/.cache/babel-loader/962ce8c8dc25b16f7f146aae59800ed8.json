2EA,IAAA,gBA3EA,4BA2EA,QA3EA,EA2EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KA/EA;AAgFA,IAAA,mBAhFA,+BAgFA,OAhFA,EAgFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KApFA;AAqFA,IAAA,GArFA,iBAqFA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,mCAAA;AACA,KAvFA;AAwFA,IAAA,wBAxFA,sCAwFA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AA3FA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <div style=\"margin: 10px 0\">\n      <el-input style=\"width: 200px\" placeholder=\"请输入昵称\" suffix-icon=\"el-icon-search\" v-model=\"nickname\"></el-input>\n      <el-input style=\"width: 200px\" placeholder=\"请输入学号\" suffix-icon=\"el-icon-search\" class=\"ml-5\" v-model=\"stuNum\"></el-input>\n      <el-input style=\"width: 200px\" placeholder=\"请输入手机号\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"phone\"></el-input>\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\n    </div>\n\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\n      <el-popconfirm\n          class=\"ml-5\"\n          confirm-button-text='确定'\n          cancel-button-text='我再想想'\n          icon=\"el-icon-info\"\n          icon-color=\"red\"\n          title=\"您确定批量删除这些数据吗？\"\n          @confirm=\"delBatch\"\n      >\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n      </el-popconfirm>\n\n      <el-upload action=\"http://localhost:8088/user/import\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\n        <el-button type=\"primary\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\n      </el-upload>\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button>\n    </div>\n\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"140\"></el-table-column>\n      <el-table-column prop=\"stuNum\" label=\"学号\" width=\"120\"></el-table-column>\n      <el-table-column prop=\"pwd\" label=\"密码\"></el-table-column>\n      <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\n      <el-table-column prop=\"grade\" label=\"年级\"></el-table-column>\n      <el-table-column prop=\"avatar\" label=\"头像\"></el-table-column>\n      <el-table-column label=\"操作\"  width=\"200\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\n          <el-popconfirm\n              class=\"ml-5\"\n              confirm-button-text='确定'\n              cancel-button-text='我再想想'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"您确定删除吗？\"\n              @confirm=\"del(scope.row.stuNum)\"\n          >\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"padding: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[2, 5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\" \n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\n      <el-form label-width=\"80px\" size=\"small\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"学号\">\n          <el-input v-model=\"form.stuNum\" autocomplete=\"off\"></el-input>\n        </el-form-item>  \n        <el-form-item label=\"密码\">\n          <el-input v-model=\"form.pwd\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"电话\">\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"年级\">\n          <el-input v-model=\"form.grade\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n         <el-form-item label=\"头像\">\n          <el-input v-model=\"form.avatar\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"User\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      nickname: \"\",\n      stuNum:\"\",\n      pwd: \"\",\n      grade: \"\",\n      phone:\"\",\n      avatar:\"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          nickname: this.nickname,\n          stuNum: this.stuNum,\n          pwd: this.pwd,\n          phone:this.phone,\n          grade:this.grade,\n          avatar:this.avatar\n        }\n      }).then(res => {\n        console.log(res)\n        // console.log(\"hhhh\");\n        this.tableData = res.data.records\n        this.total = res.data.total\n\n      })\n    },\n    save() {\n      this.request.post(\"/user/save\", this.form).then(res => {\n        console.log(res);\n        if (res.code === \"200\") {\n          this.$message.success(\"保存成功\")\n          this.dialogFormVisible = false\n          this.load()\n        } else {\n          this.$message.error(\"保存失败\")\n        }\n      })\n    },\n    handleAdd() {\n      this.dialogFormVisible = true\n      this.form = {}\n    },\n    handleEdit(row) {\n      this.form = row\n      this.dialogFormVisible = true\n    },\n    del(stuNum) {\n      this.request.delete(\"/user/\" + stuNum).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\")\n          this.load()\n        } else {\n          this.$message.error(\"删除失败\")\n        }\n      })\n    },\n    handleSelectionChange(val) {\n      console.log(val)\n      this.multipleSelection = val\n    },\n    delBatch() {\n      let stuNums = this.multipleSelection.map(v => v.stuNum)  // [{}, {}, {}] => [1,2,3]\n      this.request.post(\"/user/batch\", stuNums).then(res => {\n        if (res) {\n          this.$message.success(\"批量删除成功\")\n          this.load()\n        } else {\n          this.$message.error(\"批量删除失败\")\n        }\n      })\n    },\n    reset() {\n      this.nickname = \"\"\n      this.stuNum = \"\"\n      this.pwd = \"\"\n      this.phone = \"\"\n      this.grade = \"\"\n      this.avatar = \"\"\n      this.load()\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize)\n      this.pageSize = pageSize \n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum)\n      this.pageNum = pageNum \n      this.load()\n    },\n    exp() {\n      window.open(\"http://localhost:8088/user/export\")\n    },\n    handleExcelImportSuccess() {\n      this.$message.success(\"导入成功\")\n      this.load()\n    }\n  }\n}\n</script>\n\n\n<style>\n.headerBg {\n  background: #eee!important;\n}\n</style>\n"],"sourceRoot":"src/views"}]}