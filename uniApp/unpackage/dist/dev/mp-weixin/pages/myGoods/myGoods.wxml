<view><scroll-view style="{{'height:'+(scrollHeight)+';'}}" scroll-y="{{true}}" enable-back-to-top="{{true}}"><view class="goodsList"><block wx:for="{{Goods}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="goods_list"><view class="{{['goodsWarp','list',item.isTouchMove?'touch-move-active':'']}}" data-id="{{item.goodsId}}" data-event-opts="{{[['touchstart',[['touchStart',['$event']]]],['touchmove',[['touchMove',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e"><view class="txt"><image class="goodsImg" mode="aspectFill" src="{{item.goodsImg}}"></image><view class="goodRoot"><view class="goodsTitle">{{item.goodsName}}</view><view class="containStepper"><view class="goodsPhone">{{"联系方式: "+item.contactInfo}}</view><view class="goodsTime">{{"发布时间"+item.time}}</view></view></view></view><view class="del" data-index="{{item.goodsId}}" data-event-opts="{{[['tap',[['delGoods',['$event']]]]]}}" bindtap="__e">删除</view></view></block></block></view></scroll-view></view>